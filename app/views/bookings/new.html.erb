<div class="container" id="title">
  <div class="row justify-content-center">
    <div class="col">
      <h3>Booking Request For</h3>
      <h4><strong><%= @book.title %></strong> </h4>

      <%= simple_form_for([@book, @booking]) do |f| %>
          <%= f.input :start_date %>
          <%= f.input :end_date %>
          <%= f.button :submit, class: "btn btn-primary" %>
        <% end %>
    </div>
  </div>
</div>
